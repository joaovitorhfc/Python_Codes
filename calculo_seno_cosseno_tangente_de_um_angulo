import math
ang = float(input('Digite um 창ngulo que desejar: '))
seno = math.sin(math.radians(ang))an
print ('O 창ngulo de {} tem o seno de {:.3f}'.format(ang,seno))
cosseno = math.cos(math.radians(ang))
print ('O 창ngulo de {} tem o seno de {:.3f}'.format(ang,cosseno))
tangente = math.tan(math.radians(ang))
print ('O 창ngulo de {} tem a tangente de {:.3f}'.format(ang,tangente))
